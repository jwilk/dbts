import sys

from lib import cli

def require_python(*version):
    if sys.version_info < version:
        version_str = '.'.join(str(x) for x in version)
        message = 'Python >= {0} is required'.format(version_str)
        raise RuntimeError(message)

if __name__ == '__main__':
    require_python(3, 3)
    cli.main()

